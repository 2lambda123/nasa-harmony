{
    "compilerOptions": {
      "target": "es5",
      "lib": ["es5", "dom"],
      "types": ["cypress", "node"],
      "module": "commonjs",
      "baseUrl": ".",
      "moduleResolution": "node",
      "esModuleInterop": true,
      "allowJs": true,
      "noImplicitAny": false,
      "resolveJsonModule": true,
      "sourceMap": true,
      "outDir": "built"
    },
    "include": ["**/*.ts"],
    "esModuleInterop": true,
  }
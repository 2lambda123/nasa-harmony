  "description": "Data services frontend",
  "main": "index.js",
  "scripts": {
    "test": "eslint --ext .ts . && nyc mocha && npm audit",
    "coverage": "nyc mocha",
    "start": "ts-node -r tsconfig-paths/register app/server.ts",
    "start-dev": "supervisor app/server.js",
    "build-image": "bin/build-image",
    "push-image": "bin/push-image",
    "build": "tsc",
    "copy-files": "copyfiles 'app/schemas/**/*' 'app/templates/**/*' 'app/frontends/templates/**/*' 'config/**/*' 'db/**/*' 'public/**/*' built"
  },
  "keywords": [
    "NASA",
    "EOSDIS",
    "Harmony",
    "Services",
    "Cumulus"
  ],
  "engines": {
    "node": "^12.14.1"
  },
  "nyc": {
    "all": true,
    "check-coverage": true,
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "lines": 70,
    "reporter": [
      "lcov",
      "text"
    ],
    "include": [
      "app/**/*.ts",
      "example/**/*.ts"
    ]
  },

{
  "name": "service-wrapper",
  "version": "1.0.0",
  "description": "",
  "main": "index.ts",
  "scripts": {
    "start": "ts-node app/server.ts",
    "test": "eslint --ext .ts . && nyc mocha && npm audit --production",
    "test-fast": "TS_NODE_TRANSPILE_ONLY=true mocha",
    "lint": "eslint --ext .ts .",
    "coverage": "nyc mocha",
    "build-query-cmr-wrapper": "tsc && docker build --build-arg BASE_IMAGE=harmonyservices/query-cmr -t harmonyservices/query-cmr-wrap:${VERSION:-latest} -f Dockerfile.queryCmr .",
    "build-service-example-wrapper": "tsc && docker build --build-arg BASE_IMAGE=harmonyservices/service-example --tag harmonyservices/service-example-wrap:${VERSION:-latest} -f Dockerfile.harmony-service-example .",
    "build-netcdf-to-zarr-wrapper": "tsc && docker build --build-arg BASE_IMAGE=harmonyservices/netcdf-to-zarr --tag harmonyservices/netcdf-to-zarr-wrap:${VERSION:-latest} -f Dockerfile.netcdf-to-zarr .",
    "build-swot-repr-wrapper": "tsc && docker build --build-arg BASE_IMAGE=sds/swot-reproject --tag sds/swot-reproject-wrap:${VERSION:-latest} -f Dockerfile.swot-reproject .",
    "build-old": "npm run build-query-cmr-wrapper && npm run build-service-example-wrapper && npm run build-netcdf-to-zarr-wrapper",
    "build": "tsc && docker build --tag harmonyservices/service-runner:${VERSION:-latest} -f Dockerfile.service-runner ."
  },
  "author": "NASA EOSDIS Harmony team",
  "license": "Apache-2.0",
  "dependencies": {
    "@kubernetes/client-node": "^0.15.1",
    "agentkeepalive": "^4.1.4",
    "aws-sdk": "^2.906.0",
    "axios": "^0.21.1",
    "date-fns": "^2.21.3",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "knex": "^0.95.9",
    "knex-paginate": "^2.2.0",
    "lodash": "^4.17.21",
    "lodash.camelcase": "^4.3.0",
    "node-fetch": "^2.6.1",
    "python-shell": "^3.0.0",
    "sqlite3": "^4.2.0",
    "superagent": "^6.1.0",
    "tmp": "^0.2.1",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/axios-mock-adapter": "1.10.0",
    "@types/chai": "^4.2.11",
    "@types/chai-as-promised": "^7.1.3",
    "@types/express": "^4.17.12",
    "@types/mocha": "^7.0.2",
    "@types/sinon": "^9.0.4",
    "@types/superagent": "^4.1.8",
    "@types/supertest": "^2.0.10",
    "axios-mock-adapter": "^1.20.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "mocha": "^9.0.0",
    "sinon": "^7.5.0",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.4"
  }
}
